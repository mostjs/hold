{
  "name": "@most/hold",
  "version": "3.0.1",
  "description": "Emit latest event to new observers",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "type-definitions/hold.d.ts",
  "files": [
    "dist",
    "type-definitions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mostjs/hold.git"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:flow",
    "test:unit": "nyc --reporter=text-summary mocha -r buba/register --reporter dot --recursive 'test/**/*-test.js'",
    "test:lint": "jsinspect src/*.js test/*.js && standard --fix 'src/**/*.js' 'test/**/*.js'",
    "test:flow": "flow check",
    "build": "npm run build:dist && npm run build:min && npm run build:flow",
    "build:dist": "rollup -c",
    "build:min": "uglifyjs dist/index.js -m -o dist/index.min.js",
    "build:flow": "cpy src/index.js dist --rename=index.js.flow",
    "prepublish": "npm run build"
  },
  "keywords": [
    "most",
    "reactive",
    "event",
    "functional",
    "async"
  ],
  "author": "brian@hovercraftstudios.com",
  "license": "MIT",
  "devDependencies": {
    "@briancavalier/assert": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "buba": "^4.0.1",
    "cpy-cli": "^2.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "flow-bin": "^0.95.1",
    "jsinspect": "^0.12.7",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rollup": "^1.6.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-flow": "^1.1.1",
    "standard": "^12.0.1",
    "uglify-js": "^3.1.1"
  },
  "dependencies": {
    "@most/core": "^1.3.4",
    "@most/scheduler": "^1.2.2",
    "@most/types": "^1.0.1"
  },
  "babel": {
    "presets": [
      "env",
      "flow"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
